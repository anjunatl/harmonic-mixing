    "deploy": "gh-pages -d build -o github",
    "ci": "CI=true npm run build && npm run test"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "ajv": "^6.12.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "gh-pages": "^2.2.0",
    "http-server": "^0.12.3",
    "sass": "^1.0",
    "typescript": "^3.9.7",
    "underscore": "^1.11.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/index.tsx",
      "!**/react-app-env.d.ts",
      "!**/registerServiceWorker.js",
      "!**/node_modules/"
    ]
  },
